<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        >
        <rotate android:fromDegrees="45"
            android:toDegrees="45">
            <shape android:shape="rectangle">

                <gradient
                    android:startColor="#FFFFFF"
                    android:endColor="#f1e5d9" />

                <stroke
                    android:width="2dp"
                    android:color="#3ad9c9" />

                <size android:height="45dp"
                    android:width="45dp"
                    />
            </shape>
        </rotate>
    </item>

    <item
        android:left="8dp"
        android:top="80dp"
        android:right="8dp"
        android:bottom="80dp">

            <shape android:shape="rectangle">

                <gradient
                    android:startColor="#FFFFFF"
                    android:endColor="#f1e5d9" />

                <stroke
                    android:width="2dp"
                    android:color="#3ad9c9" />

                <size android:height="45dp"
                    android:width="45dp"
                    />
            </shape>
    </item>

</layer-list>